fn main:void
	println("Welcome to Guess the Number")
	println("Type a guess to continue.")
	declare(integer, x, 0)
	random(10) x

	declare(integer, input, -1)
	while x != input
		if x != input
			getinput() input
			if x < input
				println("Your guess is too high.")
			end
			elseif x > input
				println("Your guess is too low.")
			end
		end
	end

	println("You won the game")
end main