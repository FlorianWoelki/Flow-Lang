method main:void
	print("Welcome to Guess the Number")
	print("Type a guess to continue.")
	declare(integer, x, 0)
	random(10) x

	declare(integer, input, -1)
	while notequals x input
		if notequals x input
			getinput() input
			if lessthan x input
				print("Your guess is too high.")
			end
			elseif greaterthan x input
				print("Your guess is too low.")
			end
		end
	end

	print("You won the game")
end main